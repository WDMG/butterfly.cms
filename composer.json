        "wdmg/yii2-options": "^1.3.7",
        "wdmg/yii2-rbac": "^1.1.2",
        "wdmg/yii2-reposts": "^0.0.4",
        "wdmg/yii2-reviews": "^0.0.5",
        "wdmg/yii2-services": "^1.1.3",
        "wdmg/yii2-stats": "^1.1.3",
